Cell["Determinants", "Subsubsection",ExpressionUUID->"f2c96171-4cca-4d77-b34c-8ebdcfc410f0"],

Cell["\<\
For 3*3 matrices, the terms involving the diagonal matrix elements are \
straightforward:
(product of diagonal elements) - sum of (diagonal element) * (norm of \
opposite off-diagonal element).

But the product of the three off-diagonal elements is more difficult, because \
lack of commutativity and associativity makes their definition ambiguous. \
This function tests the maximal-symmetry combination of them\
\>", "Text",ExpressionUUID->"c052f77c-0dab-4daf-9d7c-ad6002ffc6b4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ThreeDetOffDiagProdTest", "[", 
   RowBox[{
    RowBox[{"np_", ":", "Integer"}], ",", "x_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "n", ",", "vals", ",", "prodfunc", ",", "res", ",", "scls", ",", "vecs", 
      ",", "est"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", " ", "=", " ", 
      RowBox[{"2", "^", "np"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vals", " ", "=", " ", 
      RowBox[{"Array", "[", 
       RowBox[{"x", ",", 
        RowBox[{"{", 
         RowBox[{"3", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"prodfunc", "[", 
       RowBox[{"x1_", ",", "x2_", ",", "x3_"}], "]"}], " ", ":=", " ", 
      RowBox[{"First", "[", 
       RowBox[{
        RowBox[{"CDMult", "[", 
         RowBox[{
          RowBox[{"CDMult", "[", 
           RowBox[{"x1", ",", "x2"}], "]"}], ",", "x3"}], "]"}], " ", "+", 
        " ", 
        RowBox[{"CDMult", "[", 
         RowBox[{"x1", ",", 
          RowBox[{"CDMult", "[", 
           RowBox[{"x2", ",", "x3"}], "]"}]}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", " ", "=", " ", 
      RowBox[{"Expand", "[", 
       RowBox[{
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{"prodfunc", " ", "@@", " ", "vperm"}], ",", 
          RowBox[{"{", 
           RowBox[{"vperm", ",", 
            RowBox[{"Permutations", "[", "vals", "]"}]}], "}"}]}], "]"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "*", 
          RowBox[{"3", "!"}]}], ")"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"scls", " ", "=", " ", 
      RowBox[{"First", " ", "/@", " ", "vals"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vecs", " ", "=", " ", 
      RowBox[{"Rest", " ", "/@", " ", "vals"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"est", " ", "=", " ", 
      RowBox[{
       RowBox[{"Product", "[", 
        RowBox[{
         RowBox[{"scls", "[", 
          RowBox[{"[", "k", "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "3"}], "}"}]}], "]"}], " ", "-", " ", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"scls", "[", 
           RowBox[{"[", 
            RowBox[{"k", "+", "1"}], "]"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"vecs", "[", 
             RowBox[{"[", 
              RowBox[{
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"k", "+", "1"}], ",", "3"}], "]"}], "+", "1"}], 
              "]"}], "]"}], ".", 
            RowBox[{"vecs", "[", 
             RowBox[{"[", 
              RowBox[{
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"k", "+", "2"}], ",", "3"}], "]"}], "+", "1"}], 
              "]"}], "]"}]}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "0", ",", "2"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"res", " ", "-", " ", "est"}], " ", "//", " ", "Expand"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->
  "In[208]:=",ExpressionUUID->"cfea546a-0478-487e-84aa-fdf574ed9bba"],

